{
  "manifest_version": 3,
  "name": "Bootcamp Helper",
  "version": "1.0",
  "description": "Uma extens√£o de exemplo para o Bootcamp.",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_popup": "pages/popup.html",
    "default_icon": {
      "16": "icons/icon16.png"
    }
  },
  "permissions": [
    "storage",
    "activeTab"
  ],
  "background": {
  "service_worker": "Js/background.js"
},
  "host_permissions": [
    "*://*.google.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["*://*.google.com/search*"],
      "js": ["Js/content.js"]
    }
  ]
}